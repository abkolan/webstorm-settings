<application>
  <component name="DefaultFont">
    <option name="FONT_SIZE" value="15" />
    <option name="FONT_SCALE" value="1.25" />
  </component>
  <component name="EditorSettings">
    <option name="USE_SOFT_WRAPS" value="" />
  </component>
  <component name="TodoConfiguration">
    <pattern useCustomColors="true" pattern="\btodo\b.*">
      <option name="FOREGROUND" value="0" />
      <option name="BACKGROUND" value="b7a912" />
      <option name="FONT_TYPE" value="2" />
    </pattern>
    <pattern useCustomColors="true" pattern="\bfixme\b.*">
      <option name="FOREGROUND" value="0" />
      <option name="BACKGROUND" value="1db698" />
      <option name="FONT_TYPE" value="2" />
    </pattern>
    <pattern useCustomColors="true" pattern="\brefactor\b.*">
      <option name="FOREGROUND" value="0" />
      <option name="BACKGROUND" value="4347b7" />
      <option name="FONT_TYPE" value="2" />
    </pattern>
    <pattern icon="question" useCustomColors="true" pattern="\boptimize\b.*">
      <option name="FOREGROUND" value="0" />
      <option name="BACKGROUND" value="57b710" />
      <option name="FONT_TYPE" value="2" />
    </pattern>
    <filter name="TODOs">
      <pattern index="0" />
    </filter>
    <filter name="REFACTOR">
      <pattern index="2" />
    </filter>
    <filter name="OPTIMIZE">
      <pattern index="3" />
    </filter>
  </component>
</application>