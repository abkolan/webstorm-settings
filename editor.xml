<application>
  <component name="EditorSettings">
    <option name="USE_SOFT_WRAPS" value="" />
    <option name="IS_RIGHT_MARGIN_SHOWN" value="false" />
    <option name="ARE_LINE_NUMBERS_SHOWN" value="false" />
    <option name="ARE_GUTTER_ICONS_SHOWN" value="false" />
    <option name="IS_FOLDING_OUTLINE_SHOWN" value="false" />
    <option name="SHOW_BREADCRUMBS" value="false" />
    <option name="IS_INDENT_GUIDES_SHOWN" value="false" />
  </component>
  <component name="TodoConfiguration">
    <pattern icon="default" useCustomColors="true" case-sensitive="false" pattern="\btodo\b.*">
      <option name="FOREGROUND" value="0" />
      <option name="BACKGROUND" value="b7a912" />
      <option name="FONT_TYPE" value="2" />
    </pattern>
    <pattern icon="default" useCustomColors="true" case-sensitive="false" pattern="\bfixme\b.*">
      <option name="FOREGROUND" value="0" />
      <option name="BACKGROUND" value="1db698" />
      <option name="FONT_TYPE" value="2" />
    </pattern>
    <pattern icon="default" useCustomColors="true" case-sensitive="false" pattern="\brefactor\b.*">
      <option name="FOREGROUND" value="0" />
      <option name="BACKGROUND" value="4347b7" />
      <option name="FONT_TYPE" value="2" />
    </pattern>
    <pattern icon="question" useCustomColors="true" case-sensitive="false" pattern="\boptimize\b.*">
      <option name="FOREGROUND" value="0" />
      <option name="BACKGROUND" value="57b710" />
      <option name="FONT_TYPE" value="2" />
    </pattern>
    <filter name="TODOs">
      <pattern index="0" />
    </filter>
    <filter name="REFACTOR">
      <pattern index="2" />
    </filter>
    <filter name="OPTIMIZE">
      <pattern index="3" />
    </filter>
  </component>
</application>